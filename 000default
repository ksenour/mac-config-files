NameVirtualHost *:80
Listen 443
NameVirtualHost *:443

<Directory "/Users/wagner/Sites/umn/www//">
Allow From All
AllowOverride All
Options +Indexes
</Directory>
<VirtualHost *:80>
	ServerName "umn.loc"
	DocumentRoot "/Users/wagner/Sites/umn/www/"
ServerAlias umn.site.loc
ServerAlias umn.grad.loc
</VirtualHost>

<VirtualHost *:443>
	ServerName "umn.loc"
	DocumentRoot "/Users/wagner/Sites/umn/www/"


</VirtualHost>

